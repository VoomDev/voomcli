{
  "name": "voom",
  "description": "Voom CLI",
  "version": "0.0.0",
  "author": "voom",
  "bin": {
    "voom": "./bin/run"
  },
  "bugs": "https://github.com/voomteam/voom/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.25.1",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/voomteam/voom",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "voom",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "voomteam/voom",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
